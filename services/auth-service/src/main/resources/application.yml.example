server:
  port: 8080

spring:
  application:
    name: auth-service

  datasource:
    driver-class-name: org.postgresql.Driver
    url: jdbc:postgresql://host:5432/postgres
    username: username
    password: password

  jpa:
    hibernate:
      ddl-auto: validate
    open-in-view: false
    show-sql: false
    properties:
      hibernate:
        format_sql: true
        dialect: org.hibernate.dialect.PostgreSQLDialect


jwt:
  key: "SUPER-VERY-SECRET-KEY-VERY-SECRET-VERY-VERY"
  header: "Authorization"